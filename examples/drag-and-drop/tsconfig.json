{
    "compilerOptions": {
        "target": "es5",
        "module": "CommonJS",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "strict": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "noFallthroughCasesInSwitch": true,
        "exactOptionalPropertyTypes": true,
        "noImplicitOverride": true,
        "noImplicitReturns": true,
        "noPropertyAccessFromIndexSignature": true,
        "moduleResolution": "node",
        "declaration": false,
        "jsx": "react-jsx",
    },
    "exclude": [
        "node_modules",
    ],
    "files": [
        "src/index.tsx",
    ],
}